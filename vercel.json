{
  "cleanUrls": true,
  "routes": [
    {
      "src": "/(background.jpeg|efyoos_logo.png)",
      "dest": "/$1"
    },
    {
      "has": [{ "type": "host", "value": "manager.efyoos.com" }],
      "src": "/(.*)",
      "dest": "/dashboard"
    },
    {
      "has": [{ "type": "host", "value": "guest.efyoos.com" }],
      "src": "/(.*)",
      "dest": "/guest"
    },
    { "src": "/home",      "dest": "/home.html" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/guest",     "dest": "/guest.html" },
    { "src": "/(.*)",      "dest": "/index.html" }
  ]
}