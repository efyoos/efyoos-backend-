{
  "routes": [
    {
      "src": "/(background.jpeg)",
      "dest": "/$1"
    },
    {
      "has": [{ "type": "host", "value": "manager.efyoos.com" }],
      "src": "/(.*)",
      "dest": "/dashboard.html"
    },
    {
      "has": [{ "type": "host", "value": "guest.efyoos.com" }],
      "src": "/(.*)",
      "dest": "/guest.html"
    },
    { "src": "/home.html",      "dest": "/home.html" },
    { "src": "/dashboard.html", "dest": "/dashboard.html" },
    { "src": "/guest.html",     "dest": "/guest.html" },
    { "src": "/(.*)",           "dest": "/index.html" }
  ]
}